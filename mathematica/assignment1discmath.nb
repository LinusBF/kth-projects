 FormBox[
  RowBox[{"h", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[ClubSuit]", "\[InvisibleSpace]", "10"}], ",", 
     RowBox[{"\[HeartSuit]", "\[InvisibleSpace]", "10"}], ",", 
     RowBox[{"\[DiamondSuit]", "\[InvisibleSpace]", "K"}], ",", 
     RowBox[{"\[DiamondSuit]", "\[InvisibleSpace]", "K"}], ",", 
