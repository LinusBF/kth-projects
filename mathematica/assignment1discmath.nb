     RowBox[{"onepairQ", "(", "h", ")"}]}], "=", "false"}], ",", 
   RowBox[{
    RowBox[{"twopairsQ", "(", "h", ")"}], "=", "false"}], ",", 
   RowBox[{
    RowBox[{"threeofakindQ", "(", "h", ")"}], "=", "false"}], ",", 
