 FormBox[
  RowBox[{
   SubscriptBox["n", "StraightFlush"], "=", 
   RowBox[{
    RowBox[{
     TemplateBox[{"16","1"},
      "Binomial"], " ", 
     SuperscriptBox[
      TemplateBox[{"2","1"},
       "Binomial"], "4"]}], "=", "256"}]}], TraditionalForm], "\n", 
 FormBox[
  RowBox[{
   SubscriptBox["odds", "StraightFlush"], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      SubscriptBox["n", "StraightFlush"], 
      SubscriptBox["n", "Total"]], "\[TildeTilde]", "0.00015"}], "=", 
    RowBox[{"0.015", "%"}]}]}], TraditionalForm]}],
