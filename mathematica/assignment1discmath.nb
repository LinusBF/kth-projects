Cell[TextData[{
 StyleBox["getSample",
  FontSlant->"Italic"],
 " generates ",
 StyleBox["i",
  FontSlant->"Italic"],
 " amount of random hands. Then increases the PAIR global variable for each \
random hand that is a pair."
}], "Text",
 CellChangeTimes->{{3.7458352246653323`*^9, 
  3.7458353330395775`*^9}},ExpressionUUID->"c67bd90c-8b1f-423b-8618-\
7f0037105e1b"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getSample", " ", "[", "i_", "]"}], ":=", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"sSize", "=", "0"}], ",", 
     RowBox[{"sSize", "<", "i"}], ",", 
     RowBox[{"sSize", "++"}], ",", 
     RowBox[{"getResult", "[", 
      RowBox[{"handsF", "[", 
       RowBox[{"[", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", " ", 
           RowBox[{"Length", "[", "handsF", "]"}]}], "}"}], "]"}], "]"}], 
       "]"}], "]"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{
  3.7458353755772953`*^9},ExpressionUUID->"c24091ec-e739-481e-83a2-\
dea0ff3a4824"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"Runs", " ", 
    StyleBox["getSample",
     FontSlant->"Italic"], " ", "100", " ", "times", " ", "to", " ", 
    "generate", " ", "100", " ", "samples", " ", "of", " ", "i", "*", "10000",
     " ", "hands"}], ",", " ", 
   RowBox[{"where", " ", 
    StyleBox["i",
     FontSlant->"Italic"], " ", "goes", " ", "from", " ", "1", " ", "to", " ",
     "100.", " ", "It", " ", "then", " ", "appends", " ", "the", " ", "odds", 
    " ", "of", " ", "getting", " ", 
    StyleBox["a",
     FontSlant->"Plain"], 
    StyleBox[" ",
     FontSlant->"Plain"], 
    StyleBox["pair",
     FontSlant->"Plain"], 
    StyleBox[" ",
     FontSlant->"Plain"], 
    StyleBox["in",
     FontSlant->"Plain"], 
    StyleBox[" ",
     FontSlant->"Plain"], 
    StyleBox["that",
     FontSlant->"Plain"], 
    StyleBox[" ",
     FontSlant->"Plain"], 
    StyleBox["sample",
     FontSlant->"Plain"], 
    StyleBox[" ",
     FontSlant->"Plain"], 
    StyleBox[
     RowBox[{"size", "."}],
     FontSlant->"Plain"]}]}], TraditionalForm]], "Text",
 CellChangeTimes->{{3.7458353424141655`*^9, 3.745835351282523*^9}, {
  3.745835397778475*^9, 3.7458354598864946`*^9}, {3.745835496487506*^9, 
  3.7458355808466682`*^9}},ExpressionUUID->"652a4572-4d51-4948-8e8e-\
a1ad7c2b3342"],

Cell[BoxData[
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "\[LessEqual]", "100"}], ",", 
    RowBox[{"i", "++"}], ",", 
    RowBox[{
     RowBox[{"PAIR", "=", "0"}], ";", 
     RowBox[{"getSample", "[", 
      RowBox[{"10000", "*", "i"}], "]"}], ";", 
     RowBox[{"Print", "[", "i", "]"}], ";", 
     RowBox[{"currentOdds", "=", 
      RowBox[{
       RowBox[{"PAIR", "/", "10000"}], "*", "i"}]}], ";", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"oddsStore", ",", " ", "currentOdds"}], "]"}]}]}], "]"}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.7458353804108334`*^9, 
  3.745835381960333*^9}},ExpressionUUID->"9bb35171-9116-4b1e-9879-\
187e350a0c09"],

